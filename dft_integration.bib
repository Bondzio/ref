@ARTICLE{Deglmann2002511,
  author = {Peter Deglmann and Filipp Furche and Reinhart Ahlrichs},
  title = {An efficient implementation of second analytical derivatives for
	density functional methods},
  journal = {Chemical Physics Letters},
  year = {2002},
  volume = {362},
  pages = {511 - 518},
  number = {5-6},
  abstract = {We present an implementation of analytical second-order geometric
	derivatives for density functional methods using Gaussian basis sets.
	Key features include a stable and efficient numerical quadrature,
	the direct iterative solution of the coupled perturbed Kohn–Sham
	equations, integral prescreening based on rigorous estimates, and
	exploitation of point group symmetry for all finite groups. Benchmark
	results indicate a moderate cubic growth of CPU and storage requirements
	with system size; low symmetry molecules with up to 100 heavy atoms
	can be treated on personal computers. The performance of gradient
	corrected functionals in predicting IR spectra of larger molecules
	is exemplified for transition metal carbonyl complexes.},
  doi = {DOI: 10.1016/S0009-2614(02)01084-9},
  file = {:mine/my_literature/chemistry/quantum_chemistry/DFT/integration/Second_Derivative_DFT_For_Turbomole.pdf:PDF},
