@ARTICLE{johnson:5612,
  author = {Benny G. Johnson and Peter M. W. Gill and John A. Pople},
  title = {The performance of a family of density functional methods},
  journal = {The Journal of Chemical Physics},
  year = {1993},
  volume = {98},
  pages = {5612-5626},
  number = {7},
  abstract = {The results of a systematic study of molecular properties by density
	functional theory (DFT) are presented and discussed. Equilibrium
	geometries, dipole moments, harmonic vibrational frequencies, and
	atomization energies were calculated for a set of 32 small neutral
	molecules by six different local and gradient-corrected DFT methods,
	and also by the ab initio methods Hartree–Fock, second-order Møller–Plesset,
	and quadratic configuration interaction with single and double substitutions
	(QCISD). The standard 6-31G* basis set was used for orbital expansion,
	and self-consistent Kohn–Sham orbitals were obtained by all DFT methods,
	without employing any auxiliary fitting techniques. Comparison with
	experimental results shows the density functional geometries and
	dipole moments to be generally no better than or inferior to those
	predicted by the conventional ab initio methods with this particular
	basis set. The density functional vibrational frequencies compare
	favorably with the ab initio results, while for atomization energies,
	two of the DFT methods give excellent agreement with experiment and
	are clearly superior to all other methods considered.},
  doi = {10.1063/1.464906},
  file = {:mine/my_literature/chemistry/quantum_chemistry/DFT/functional/The_Performance_of_A_Family_DFT_Methods_JCP_Pople_Gill_Johnson.pdf:PDF},
  keywords = {DENSITY FUNCTIONAL METHOD; MOLECULES; EQUILIBRIUM; GEOMETRY; DIPOLE
	MOMENTS; HARMONIC OSCILLATORS; ATOMIZATION; PERTURBATION THEORY;
	POTENTIALS},
  owner = {liufenglai},
  publisher = {AIP},
  timestamp = {2009.11.20},
  url = {http://link.aip.org/link/?JCP/98/5612/1}
}

